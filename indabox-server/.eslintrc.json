{
    "extends": "airbnb-base",
    "env": {
        "node": true
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": 0,
        "max-len": [
            2,
            120,
            4
        ]
    },
    "globals": {
        "log": true
    }
}
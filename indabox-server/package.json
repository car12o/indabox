{
  "name": "indabox-server",
  "version": "1.0.0",
  "description": "associates managements system - server",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "import": "node src/imports/csv.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/car12o/indabox.git"
  },
  "keywords": [
    "associates"
  ],
  "author": "car12o",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/car12o/indabox/issues"
  },
  "homepage": "https://github.com/car12o/indabox#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "nodemon": "^1.18.9"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "globals": {
      "log": true
    },
    "rules": {
      "no-console": 1,
      "max-len": [
        2,
        120
      ],
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "quotes": [
        2,
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "consistent": true
          },
          "ObjectPattern": {
            "consistent": true
          },
          "ImportDeclaration": {
            "consistent": true
          },
          "ExportDeclaration": {
            "consistent": true
          }
        }
      ]
    }
  }
}
